extends layout

block content
  .w3-container.w3-margin 
    table.w3-table-all.w3-card-4.w3-hoverable(id='myTable')
        tr.w3-orange
          th ID
          th Name
          th Course
        each member in dados.members
          tr
            td= member.id
            td= member.name
            td= member.course
  .w3-container.w3-deep-orange.w3-center
      a.w3-btn.w3-hover-orange.w3-sand.w3-round-xxlarge.w3-margin(href='/') Voltar ao Inicio